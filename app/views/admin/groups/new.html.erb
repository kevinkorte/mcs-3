<div class="container">
  <div class="row">
    <div class="col-md-12">
      
      <%= simple_form_for([:admin, @group]) do |f| %>
        <%= f.error_notification %>

        <div class="form-inputs">
            <%= f.input :group %>
        </div>

        <div class="form-actions">
          <%= f.button :submit %>
        </div>
      <% end %>

    </div>
  </div>
</div>